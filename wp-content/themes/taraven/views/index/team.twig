<div class="wpex-vc-row-wrap clr wpex-vc-row-centered">

    <div data-ls_id="#home-equipe" class="vc_row wpb_row vc_row-fluid vc_custom_1444182072304">

        <div class="container center-row clr">

            <div class="wpex-vc-columns-wrap clr">
                <div class="wpb_column vc_column_container vc_col-sm-12">
                    <div class="vc_column-inner wpex-clr">
                        <div class="wpb_wrapper wpex-vc-column-wrapper wpex-clr ">
                            <h2 style="font-size: 32px;text-align: center;font-family:Roboto Slab;font-weight:400;font-style:normal" class="vc_custom_heading">{{site.acf.equipe_titulo}}</h2>
                            <div class="vcex-divider vcex-divider-solid" style="width:10%;margin-top:15px;margin-bottom:25px;border-top-width:3px;border-color:#ef5c32;"></div>
                            <div class="wpb_text_column wpb_content_element ">
                                <div class="wpb_wrapper">{{site.acf.equipe_conteudo}}</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- .wpex-vc-columns-wrap -->

        </div>
        <!-- .container -->



    </div>


</div>
<!-- .wpex-vc-row-wrap -->


<div class="wpex-vc-row-wrap clr wpex-vc-row-centered wpex-vc-has-custom-column-spacing wpex-vc-column-spacing-30 wpex-offset-vc-15">

    <div class="vc_row wpb_row vc_row-fluid" style="padding-bottom: 40px;">

        <div class="container center-row clr">

            <div class="wpex-vc-columns-wrap clr">
                {% set image_default = site.theme.link ~'/assets/img/image-available.png' %}
              {% for item in site.acf.equipe_itens %}
                <div class="wpb_column vc_column_container vc_col-sm-4">
                    <div class="vc_column-inner wpex-clr">
                        <div class="wpb_wrapper wpex-vc-column-wrapper wpex-clr ">
                            <div class="vcex-icon-box clr vcex-icon-box-four" style="border-color:#dddddd;">
                                <img src="{{item.foto|default(image_default)}}" alt="{{item.nome}}" class="vcex-icon-box-image" width="150" style="width:150px;" />

                                <h2 class="vcex-icon-box-heading">{{item.nome}}</h2>
                                <div class="vcex-icon-box-content clr">
                                    <p>{{item.descricao}}</p>
                                </div>
                                <!-- .vcex-icon-box-content -->
                            </div>
                            <!-- .vcex-icon-box -->
                        </div>
                    </div>
                </div>
              {% endfor %}
            </div>
            <!-- .wpex-vc-columns-wrap -->

        </div>
        <!-- .container -->



    </div>


</div>
<!-- .wpex-vc-row-wrap -->