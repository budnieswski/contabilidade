    <a href="#" class="scrollup">GoUp</a>
    
    <div class="row">
      <a href="{{ fn('get_permalink', 194) }}" title="Trabalhe Conosco">
        <img src="{{theme.link}}/assets/img/footer-trabalhe-conosco.png" alt="Trabalhe Conosco" title="Trabalhe Conosco">
      </a>
      <a href="https://www.facebook.com/muitomaisdigital" target="_blank" title="Facebook Muito Mais Digital">
        <img src="{{theme.link}}/assets/img/footer-facebook.png" alt="Facebook Muito Mais Digital" title="Facebook Muito Mais Digital">
      </a>
    </div>

    <div class="row">
      <div itemscope itemtype="http://schema.org/LocalBusiness">
        <span itemprop="address" itemscope itemtype="http://schema.org/PostalAddress">
          <span itemprop="streetAddress">Av. Marechal Floriano Peixoto - 7.401 - Sala 9 - Hauer</span> |
          <span itemprop="addressLocality">Curitiba</span> -
          <span itemprop="addressRegion">PR</span> |
          <span itemprop="postalCode">81.650-000</span> |
        </span>
          <span itemprop="telephone">41 3042-0444</span> |
          <a href="mailto:falecom@muitomaisdigital.com.br" itemprop="email">falecom@muitomaisdigital.com.br</a>
      </div>
    </div>

  </div> {# end container #}
</div> {# end footer #}



{# JS
================================================== #}
{% for js in site.theme.js %}
  {% if js matches '{^(http|https|//)}' %}
    <script src="{{ js }}"></script>
  {% else %}
    <script src="{{ site.theme.link }}/assets/js/{{ js }}"></script>
  {% endif %}
{% endfor %}

