<head>
  <meta charset="utf-8">
  <title>{{ fn('wp_title') }}</title>
  <!-- <meta name="author" content="Guilherme Budnieswski - Muito Mais Digital"> -->
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

  <!--[if lt IE 9]> <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script> <![endif]-->

  {# Favicons
  ================================================== #}
  <link rel="shortcut icon" href="{{ site.theme.link }}/assets/img/favicon/favicon.ico" type="image/x-icon" />
  <link rel="apple-touch-icon" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon.png" />
  <link rel="apple-touch-icon" sizes="57x57" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-57x57.png" />
  <link rel="apple-touch-icon" sizes="72x72" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-72x72.png" />
  <link rel="apple-touch-icon" sizes="76x76" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-76x76.png" />
  <link rel="apple-touch-icon" sizes="114x114" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-114x114.png" />
  <link rel="apple-touch-icon" sizes="120x120" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-120x120.png" />
  <link rel="apple-touch-icon" sizes="144x144" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-144x144.png" />
  <link rel="apple-touch-icon" sizes="152x152" href="{{ site.theme.link }}/assets/img/favicon/apple-touch-icon-152x152.png" />

  {{wp_head}}

  {# Browser Update
  ================================================== #}
  <script> 
  var $buoop = {c:2}; 
  function $buo_f(){ 
   var e = document.createElement("script"); 
   e.src = "//browser-update.org/update.js"; 
   document.body.appendChild(e);
  };
  try {document.addEventListener("DOMContentLoaded", $buo_f,false)}
  catch(e){window.attachEvent("onload", $buo_f)}
  </script> 
</head>

<body id="blog">
  
{# Header
================================================== #}
<div class="band header">
  <div class="container">

      <div class="three columns logo"> <a href="{{ site.url }}/blog" class="block">LOGO</a> </div>

      <div class="two columns u-remove-left google-partner">
        <a href="https://www.google.com/partners/?hl=pt-BR#a_profile;idtf=6313788996;" target="_blank" title="Agência Parceira Google">
          <img src="{{theme.link}}/assets/img/logo-google-partner.png" alt="Agência Parceira Google" title="Agência Parceira Google">
        </a>
      </div>

      <div class="four columns afins u-pull-right">
        <div class="wrapper">
          <div class="telefone">(41) 3042-0444 | (41) 3046-0449</div>
          <div class="voltar-site">
            <a href="{{ site.url }}" title="Volte para o site">
              <img class="u-max-full-width" src="{{ theme.link }}/assets/img/blog/volte-para-o-site.png" alt="Volte para o site" title="Volte para o site">
            </a>
          </div>
        </div>
      </div> {# afins #}      

  </div> <!-- e#container -->
</div> <!-- e#header -->


{# Menu
================================================== #}
<div class="band menu">
  <div class="container">
    
    <nav class="twelven columns align-center">
      <div class="separator"></div>
      {{ macros.menu(site.menu.blog.get_items, '') }}        
    </nav>

  </div> <!-- e#container -->
</div> <!-- e#menu -->


{# Newsletter
================================================== #}
<div class="band newsletter">
  <div class="spacer"></div>
  <div class="container">
    <div class="row">
      <div class="columns one-half">
        <h3>Não perca as próximas atualizações,<br>assine nossa newsletter</h3>
      </div>
      <div class="columns one-half">
        <form method="post" action="{{site.link}}/wp-content/plugins/newsletter/do/subscribe.php">
          <div class="columns seven">
            <div class="row"> <input type="text" name="nn" required="required" placeholder="NOME"> </div>
            <div class="row"> <input type="text" name="ne" required="required" placeholder="E-MAIL"> </div>
          </div>
          <div class="columns five"><input type="submit" value="INSCREVA-SE AGORA" class="enviar"></div>
        </form>
      </div>
    </div>
  </div> <!-- e#container -->
</div> <!-- e#newsletter -->


{# Wrap
================================================== #}
<div class="band wrap">
  <div class="container">    
    
    <div class="row">
      <div class="eight columns">
      {% block content %}{% endblock %}
      </div> <!-- e#conteudo -->
      
      <div class="four columns u-remove-left u-pull-right sidebar">{{ get_sidebar('blog/sidebar') }}</div>
      <div class="u-cf"><br></div>
    </div>

    {# <div class="home">{% include 'index.fullblock-contato.twig' %}</div> #}


  </div> <!-- e#container -->
</div> <!-- e#wrap -->

{# Newsletter
================================================== #}
<div class="band newsletter">
  <div class="container">
    <div class="row">
      <div class="columns one-half">
        <h3>Não perca as próximas atualizações,<br>assine nossa newsletter</h3>
      </div>
      <div class="columns one-half">
        <form method="post" action="{{site.link}}/wp-content/plugins/newsletter/do/subscribe.php">
          <div class="columns seven">
            <div class="row"> <input type="text" name="nn" required="required" placeholder="NOME"> </div>
            <div class="row"> <input type="text" name="ne" required="required" placeholder="E-MAIL"> </div>
          </div>
          <div class="columns five"><input type="submit" value="INSCREVA-SE AGORA" class="enviar"></div>
        </form>
      </div>
    </div>
  </div> <!-- e#container -->
  <div class="spacer larger"></div>
</div> <!-- e#newsletter -->



{# Footer
================================================== #}
